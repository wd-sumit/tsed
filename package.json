    "@tsed/socketio": "^6.113.2",
    "@tsed/swagger": "^6.113.2",
    "ajv": "^8.11.0",
    "apollo-datasource": "^3.3.1",
    "apollo-datasource-rest": "^3.5.2",
    "apollo-server-core": "^3.6.7",
    "apollo-server-express": "2.25.2",
    "barrelsby": "^2.3.4",
